<body>
    <?php
    $a=array("Tokyo", "Mexico City", "New York City", "Mumbai", "Seoul", "Shanghai", "Lagos", "Buenos Aires", "Cairo", "London");    
    foreach($a as $item){
        echo $item;   
    }
    echo"<br>";
    sort($a);
    ?>
    <ul>

    <?php
    for($i=0;$i<count(value: $a);$i++){
        echo "<li>".$a[$i]."</li>";
    }
    array_push($a,"Los Angeles","Calcutta","Osaka","Beijing");
    echo "<br>";
    ?>
</ul>
<ul>
    <?php
    for($i=0;$i<count(value:$a);$i++){
        echo"<li>".$a[$i]."</li>";
    }
    ?>
</ul>
